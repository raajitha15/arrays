class Merge_Sort 
{
	public static void main(String[] args) 
	{
		int a[]={1,4,2,3,7};
		int b[]={6,8,9,5};
		int res[]=new int[a.length+b.length];
		for(int i=0;i<a.length;i++)
		{
			res[i]=a[i];
		}
		for(int i=a.length,k=0;i<res.length;i++,k++)
		{
			res[i]=b[k];
		}
		for(int i=0;i<res.length;i++)
		{
			if(i==res.length-1)
			{
		      System.out.print(res[i]+ " ,");
			}
			else
			{
				System.out.print(res[i]+ " ,");
			}
		}
	}
}
