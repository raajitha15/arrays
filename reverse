class Reverse_Array 
{
	public static void main(String[] args) 
	{
		int arr[]={5,7,9,6,2};
		int res[]=new int[arr.length];
		for(int i=0,j=arr.length-1;i<arr.length;i++,j--)
		{
			res[i]=arr[j];
		}
		for(int i=0;i<arr.length;i++)
		{
		System.out.println("reversed elements are " + res[i]);
		}
	}
}
