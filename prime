//WAP to print prime numbers
class Prime_Array 
{
	public static void main(String[] args) 
	{
		int a[]={6,4,9,2,5};
		for(int i=0;i<a.length;i++)
		{
			int count=0;
			for(int j=2;j<a[i]/2;j++)
			{
				if(a[i]%j==0)
				{
					count++;
				}
			}
			if(count==0)
			{

		      System.out.println("cube numbers are "+a[i]*a[i]*a[i]);
			}
			else if(a[i]%2==0)
			{
				System.out.println("square numbers are "+a[i]*a[i]);
			}
		}
	}
}
